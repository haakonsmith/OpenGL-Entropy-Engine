#version 330 core

// Interpolated values from the vertex shaders
in vec3 UV;

// Output data
out vec4 color;

uniform sampler2D texSampler;
uniform sampler2D texSampler1;

void main()
{
	color = vec4(texture( texSampler, UV.xy ).rgb * texture( texSampler1, UV.xy ).a, 1);
	// color = vec4(1,1,1, texture( texSampler1, UV.xy ).a);
}